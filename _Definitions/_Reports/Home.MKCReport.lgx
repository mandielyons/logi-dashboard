                <ChartCanvasLegend />
              </ChartCanvas>
            </Remark>
            <PopupPanel Height="50" HeightScale="%" ID="popSubContracts" PopupPanelLocation="Mouse" Width="50" WidthScale="%">
              <Division ID="divSubContracts">
                <DataTable AccessibleHeaders="True" AjaxPaging="True" ID="KPI-table" SortArrows="True">
                  <Remark>
                    <InteractivePaging CaptionType="Text" HideShowPrevNextCaptions="True" HideWhenOnePage="True" Location="Top" PageRowCount="20" ShowPageNumber="True" />
                  </Remark>
                  <DataLayer ConnectionID="connMKC" ID="SQLDataLayer4" Source="Select CONTRACTNMBR,&#xD;&#xA;  Sum(REMAINING_BU) As REMAINING_BU&#xD;&#xA;From [GaugeAgTraxContracts]&#xD;&#xA;/* Where [GaugeAgTraxContracts].CUSTDESC = '@Request.paramCust~' Or '@Request.paramCust~' = 'All' */&#xD;&#xA;Group By CONTRACTNMBR&#xD;&#xA;HAVING SUM(REMAINING_BU) NOT BETWEEN 0 AND 999&#xD;&#xA;Order By REMAINING_BU ASC&#xD;&#xA;" Type="SQL" />
                  <DataTableColumn Header="CONTRACT NO" ID="colCONTRACTNMBR">
                    <Label Caption="@Data.CONTRACTNMBR~" ID="lblCONTRACTNMBR" />
                    <DataColumnSort DataColumn="CONTRACTNMBR" />
                    <Remark>
                      <Action ID="rptContractsAll" Type="Report">
                        <Target Report="DataTable-DrillDown" RequestForwarding="True" Type="Report" />
                        <LinkParams paramContract="@Request.paramContract~" />
                      </Action>
                    </Remark>
                  </DataTableColumn>
                  <DataTableColumn Header="REMAINING BUSHELS" ID="colREMAINING_BU">
                    <Label Caption="@Data.REMAINING_BU~" Format="###,###,##0" ID="lblREMAINING_BU" />
                    <DataColumnSort DataColumn="REMAINING_BU" DataType="Number" />
                  </DataTableColumn>
                  <DataTableColumn ID="colKPI">
                    <Label ID="KPI">
                      <ConditionalClass Class="fa fa-arrow-up fa-2x text-success" Condition="@Data.REMAINING_BU~&gt; 0" />
                      <ConditionalClass Class="fa fa-arrow-down fa-2x text-danger" Condition="@Data.REMAINING_BU~&lt;= 1000" />
                    </Label>
                  </DataTableColumn>
                </DataTable>
              </Division>
            </PopupPanel>
          </Division>
        </Division>
      </ResponsiveColumn>
    </ResponsiveRow>
    <ResponsiveRow CollisionBehavior="Overlap">
      <ResponsiveColumn>
        <Division Class="panel panel-default" HtmlDiv="True" ID="panel">
          <Division Class="panel-heading" HtmlDiv="True" ID="panel-heading">
            <Image Caption="calendar.png" Height="30" Width="30" />
            <Label Caption=" Customer Receipts By Commodity (YTD)" />
          </Division>
          <Division Class="panel-body" HtmlDiv="True" ID="panel-body">
            <ChartCanvas ChartHeight="300" ID="BarChartCanvas2">
              <Series ChartXDataColumn="COMMODITY" ChartYDataColumn="BU_YTD" Color="DarkOrange" Type="Bar">
                <Quicktip>
                  <QuicktipRow Caption="Commodity: " Value="@Chart.COMMODITY~" />
                  <QuicktipRow Caption="BU YTD: " Format="###,###,##0.00" Value="@Chart.BU_YTD~" />
                </Quicktip>
              </Series>
              <ChartXAxis AxisCaption="COMMODITY">
                <AxisLabelStyle FontAngle="0" />
