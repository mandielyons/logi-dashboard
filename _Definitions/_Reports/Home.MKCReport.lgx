      <ResponsiveColumn ColspanExtraSmallScreen="12" ColspanLargeScreen="3" ColspanMediumScreen="3" ColspanSmallScreen="12">
        <Division Class="kpi kpi-yellow" HtmlDiv="True" ID="div-KPI">
          <ResponsiveRow CollisionBehavior="Overlap">
            <ResponsiveColumn ColspanExtraSmallScreen="12" ColspanLargeScreen="9" ColspanMediumScreen="9" ColspanSmallScreen="12">
              <Label Caption="@Local.RemainBalanceMilo~" Class="kpi-value" Format="###,###,##0" Tooltip="Remaining Contract Bushels" />
              <Label Caption="BU Milo" Class="kpi-label" />
            </ResponsiveColumn>
            <ResponsiveColumn>
              <Image Caption="milo.png" Height="50" ID="imgMilo" Width="50" />
              <Label Class="down-arrow" />
            </ResponsiveColumn>
          </ResponsiveRow>
        </Division>
      </ResponsiveColumn>
    </ResponsiveRow>
    <ResponsiveRow CollisionBehavior="Overlap">
      <ResponsiveColumn>
        <Division Class="panel panel-default" HtmlDiv="True" ID="panel">
          <Division Class="panel-heading" HtmlDiv="True" ID="panel-heading">
            <Image Caption="warning.png" Class="fa fa-warning fa-2x text-danger" Height="28" ID="imgWarning" Width="28" />
            <Label Caption=" Contracts Overshipped" />
          </Division>
          <Division Class="panel-body" HtmlDiv="True" ID="panel-body">
            <Remark>
              <ChartCanvas ChartOrientation="SwapAxes" ID="BarChartCanvas1">
                <Series ChartXDataColumn="CUSTDESC" ChartYDataColumn="rdGrAggr-RemainBalance-sum" Color="Orange" Type="Bar">
                  <Quicktip>
                    <QuicktipRow Caption="CUSTOMER:" Value="@Chart.CUSTDESC~" />
                    <QuicktipRow Caption="REMAINING BALANCE: " Value="@Chart.rdGrAggr-RemainBalance-sum~" />
                  </Quicktip>
                  <Action ElementID="KPI-table" ID="actRefresh" Type="RefreshElement">
                    <LinkParams paramCust="@Chart.CUSTDESC~" />
                  </Action>
                </Series>
                <ChartXAxis AxisCaption="CUSTOMER">
                  <AxisLabelStyle FontAngle="40" />
                </ChartXAxis>
                <ChartYAxis AxisCaption="REMAINING BALANCE" />
                <DataLayer ConnectionID="connMKC" ID="SQLDataLayer3" Source="Select [GaugeAgTraxContracts].CUSTDESC,&#xD;&#xA;  Sum([GaugeAgTraxContracts].REMAINING_BU) As RemainBalance&#xD;&#xA;From [GaugeAgTraxContracts]&#xD;&#xA;Group By [GaugeAgTraxContracts].CUSTDESC" Type="SQL">
                  <GroupFilter DataType="Text" GroupColumn="CUSTDESC" KeepGroupedRows="False">
                    <GroupAggregateColumn AggregateColumn="RemainBalance" AggregateFunction="SUM" DataType="Number" ID="rdGrAggr-RemainBalance-sum" />
                  </GroupFilter>
                  <SortFilter DataType="Number" SortColumn="rdGrAggr-RemainBalance-sum" SortSequence="Ascending" />
                  <RelevanceFilter DataColumn="rdGrAggr-RemainBalance-sum" RelevanceType="NumberOfRows" RelevanceValue="5" />
                </DataLayer>
              </ChartCanvas>
            </Remark>
            <DataTable AccessibleHeaders="True" AjaxPaging="True" ID="Contracts" SortArrows="True">
              <DataLayer ID="dllContractsOvershipped" LinkedDataLayerID="linkContractsOvershipped" Type="Linked" />
              <InteractivePaging CaptionType="Text" HideShowPrevNextCaptions="True" HideWhenOnePage="True" Location="Top" PageRowCount="5" ShowPageNumber="True" />
              <DataTableColumn Header="Contract" ID="colCONTRACTNMBR">
                <Label Caption="@Data.CONTRACTNMBR~" ID="lblCONTRACTNMBR">
                  <Remark>
                    <Action ID="actReportDT" Type="Report">
                      <Target Report="DataTable-DrillDown" RequestForwarding="True" Type="Report" />
                      <LinkParams paramContract="@Data.CONTRACTNMBR~" />
                    </Action>
                  </Remark>
                  <Remark>
                    <Action ElementID="popSubContracts" ID="showSubContracts" Type="ShowElement" />
                  </Remark>
                  <Action ElementID="Contracts,SubContracts" ID="refSubContracts" Type="RefreshElement">
                    <LinkParams paramContract="@Data.CONTRACTNMBR~" />
                  </Action>
                </Label>
                <DataColumnSort DataColumn="CONTRACTNMBR" />
                <ConditionalClass Class="selected-row" Condition="&quot;@Request.paramContract~&quot;==&quot;@Data.CONTRACTNMBR~&quot;" ID="ccSelected" />
              </DataTableColumn>
              <Remark>
                <DataTableColumn Header="Location" ID="colLOCDESC">
                  <Label Caption="@Data.LOCDESC~" ID="lblLOCDESC" />
                  <DataColumnSort DataColumn="LOCDESC" />
                  <ConditionalClass Class="selected-row" Condition="&quot;@Request.paramContract~&quot;==&quot;@Data.CONTRACTNMBR~&quot;" ID="ccSelected" />
                </DataTableColumn>
              </Remark>
              <DataTableColumn Header="Commodity" ID="colCOMMODITYDESC">
                <Label Caption="@Data.COMMODITYDESC~" ID="lblCOMMODITYDESC" />
                <DataColumnSort DataColumn="COMMODITYDESC" />
                <ConditionalClass Class="selected-row" Condition="&quot;@Request.paramContract~&quot;==&quot;@Data.CONTRACTNMBR~&quot;" ID="ccSelected" />
              </DataTableColumn>
              <DataTableColumn Header="Remaining BU" ID="colREMAINING_BU">
                <Label Caption="@Data.REMAINING_BU~" Format="###,###,##0" ID="lblREMAINING_BU" />
                <DataColumnSort DataColumn="REMAINING_BU" DataType="Number" />
                <ConditionalClass Class="selected-row" Condition="&quot;@Request.paramContract~&quot;==&quot;@Data.CONTRACTNMBR~&quot;" ID="ccSelected" />
              </DataTableColumn>
              <DataTableColumn ID="colKPI">
                <Label ID="KPI">
                  <ConditionalClass Class="fa fa-arrow-up fa-2x text-success" Condition="@Data.REMAINING_BU~&gt; 1000" />
                  <ConditionalClass Class="fa fa-arrow-down fa-2x text-danger" Condition="@Data.REMAINING_BU~&lt;= 1000" />
                </Label>
                <ConditionalClass Class="selected-row" Condition="&quot;@Request.paramContract~&quot;==&quot;@Data.CONTRACTNMBR~&quot;" ID="ccSelected" />
              </DataTableColumn>
            </DataTable>
          </Division>
        </Division>
      </ResponsiveColumn>
      <ResponsiveColumn>
        <Division Class="panel panel-default" HtmlDiv="True" ID="panel">
          <Division Class="panel-heading" HtmlDiv="True" ID="panel-heading">
            <Image Caption="ContractSignature24x24.png" Height="24" ID="imgContractSig" Width="24" />
            <Spaces SecurityRightID="subSpace" Size="2" />
            <Label Caption=" Contracts Balance Performance" />
            <Spaces SecurityRightID="subSpace" Size="5" />
            <Label Caption=" " Class="fa fa-refresh text-success, ThemeAlignRight">
              <Action ID="actionReport" Type="Report">
                <Target Report="Home.MKCReport" Type="Report" />
              </Action>
            </Label>
          </Division>
          <Division Class="panel-body" HtmlDiv="True" ID="panel-body">
            <DataTable AccessibleHeaders="True" AccessibleSummary="DataTable" ID="SubContracts" KeepScrollPosition="True" RememberSort="True" ResizableColumns="True" SortArrows="True" TableBorder="0">
              <DataLayer ConnectionID="connMKC" ID="dlSubContracts" Source="Select [GaugeAgTraxContracts].CUSTDESC,&#xD;&#xA;  [GaugeAgTraxContracts].CUSTNMBR,&#xD;&#xA;  [GaugeAgTraxContracts].CUSTADDR1,&#xD;&#xA;  [GaugeAgTraxContracts].CUSTCITY,&#xD;&#xA;  [GaugeAgTraxContracts].CUSTSTATE,&#xD;&#xA;  [GaugeAgTraxContracts].CUSTZIPCODE,&#xD;&#xA;  [GaugeAgTraxContracts].CONTRACTNMBR,&#xD;&#xA;  [GaugeAgTraxContracts].CONTRACTDT,&#xD;&#xA;  [GaugeAgTraxContracts].DPRPOSITION,&#xD;&#xA;  [GaugeAgTraxContracts].DPRDESC,&#xD;&#xA;  [GaugeAgTraxContracts].LOCNMBR,&#xD;&#xA;  [GaugeAgTraxContracts].LOCDESC,&#xD;&#xA;  [GaugeAgTraxContracts].COMMODITYCD,&#xD;&#xA;  [GaugeAgTraxContracts].COMMODITYDESC,&#xD;&#xA;  [GaugeAgTraxContracts].ISSUED_LBS,&#xD;&#xA;  [GaugeAgTraxContracts].APPLIED_LBS,&#xD;&#xA;  [GaugeAgTraxContracts].ADJUSTED_LBS,&#xD;&#xA;  [GaugeAgTraxContracts].SUB_CONT_LBS,&#xD;&#xA;  [GaugeAgTraxContracts].REMAINING_LBS,&#xD;&#xA;  [GaugeAgTraxContracts].ISSUED_BU,&#xD;&#xA;  [GaugeAgTraxContracts].APPLIED_BU,&#xD;&#xA;  [GaugeAgTraxContracts].ADJUSTED_BU,&#xD;&#xA;  [GaugeAgTraxContracts].SUB_CONT_LBS/56 AS SUB_CONT_BU,&#xD;&#xA;  [GaugeAgTraxContracts].REMAINING_BU&#xD;&#xA;From [GaugeAgTraxContracts]&#xD;&#xA;Where [GaugeAgTraxContracts].CONTRACTNMBR LIKE '@Request.paramContract~'+'.%' Or '@Request.paramContract~' = 'ALL' &#xD;&#xA;Order by [GaugeAgTraxContracts].CONTRACTNMBR" Type="SQL">
                <SortFilter DataType="Text" ID="SortContractNumber" SortColumn="CONTRACTNMBR" SortSequence="Ascending" />
              </DataLayer>
              <InteractivePaging CaptionType="Image" Class="ThemeHeaderSmaller" HideShowPrevNextCaptions="False" HideWhenOnePage="True" IDnotSupportedHere="pagingTableNumbered" Location="Top" PageRowCount="15" ShowPageNumber="Numbered" />
              <DataTableColumn Header="Contract" ID="colCONTRACTNMBR">
                <Label Caption="@Data.CONTRACTNMBR~" ID="colCONTRACTNMBR">
                  <Remark>
                    <Action ID="actReportDT" Type="Report">
                      <Target Report="DataTable-DrillDown" RequestForwarding="True" Type="Report" />
                      <LinkParams paramContract="@Data.CONTRACTNMBR~" />
                    </Action>
                  </Remark>
                  <Action ID="rptContractsAll" Type="Report">
                    <Target FrameID="NewWindow" Report="DataTable-DrillDown" RequestForwarding="True" Type="Report" />
                    <LinkParams paramContract="@Request.paramContract~" />
                  </Action>
                </Label>
                <DataColumnSort DataColumn="CONTRACTNMBR" DataType="Text" />
              </DataTableColumn>
              <Remark>
                <DataTableColumn Header="Customer" ID="colCUSTDESC">
                  <Label Caption="@Data.CUSTDESC~" ID="colCUSTDESC" />
                  <DataColumnSort DataColumn="CUSTDESC" DataType="Text" />
                </DataTableColumn>
              </Remark>
              <Remark>
                <DataTableColumn Header="Account" ID="colCUSTNMBR">
                  <Label Caption="@Data.CUSTNMBR~" ID="colCUSTNMBR" />
                  <DataColumnSort DataColumn="CUSTNMBR" DataType="Text" />
                </DataTableColumn>
              </Remark>
              <Remark>
                <DataTableColumn Header="Date" ID="colCONTRACTDT">
                  <Label Caption="@Data.CONTRACTDT~" Format="Short Date" ID="colCONTRACTDT" />
                  <DataColumnSort DataColumn="CONTRACTDT" DataType="Text" />
                </DataTableColumn>
              </Remark>
              <Remark>
                <DataTableColumn Header="DPR" ID="colDPRPOSITION">
                  <Label Caption="@Data.DPRPOSITION~" ID="colDPRPOSITION" />
                  <DataColumnSort DataColumn="DPRPOSITION" DataType="Text" />
                </DataTableColumn>
              </Remark>
              <Remark>
                <DataTableColumn Header="Branch" ID="colLOCNMBR">
                  <Label Caption="@Data.LOCNMBR~" ID="colLOCNMBR" />
                  <DataColumnSort DataColumn="LOCNMBR" DataType="Text" />
                </DataTableColumn>
              </Remark>
              <DataTableColumn Header="Location" ID="colLOCDESC">
                <Label Caption="@Data.LOCDESC~" ID="colLOCDESC" />
                <DataColumnSort DataColumn="LOCDESC" DataType="Text" />
              </DataTableColumn>
              <Remark>
                <DataTableColumn Header="Code" ID="colCOMMODITYCD">
                  <Label Caption="@Data.COMMODITYCD~" ID="colCOMMODITYCD" />
                  <DataColumnSort DataColumn="COMMODITYCD" DataType="Text" />
                </DataTableColumn>
              </Remark>
              <DataTableColumn Header="Commodity" ID="colCOMMODITYDESC">
                <Label Caption="@Data.COMMODITYDESC~" ID="colCOMMODITYDESC" />
                <DataColumnSort DataColumn="COMMODITYDESC" DataType="Text" />
              </DataTableColumn>
              <Remark>
                <DataTableColumn Header="Issued LBS" ID="colISSUED_LBS">
                  <Label Caption="@Data.ISSUED_LBS~" ID="colISSUED_LBS" />
                  <DataColumnSort DataColumn="ISSUED_LBS" DataType="Number" />
                </DataTableColumn>
              </Remark>
              <Remark>
                <DataTableColumn Header="Remaining LBS" ID="colREMAINING_LBS">
                  <Label Caption="@Data.REMAINING_LBS~" ID="colREMAINING_LBS" />
                  <DataColumnSort DataColumn="REMAINING_LBS" DataType="Number" />
                </DataTableColumn>
              </Remark>
              <DataTableColumn Header="Remaining BU" ID="colREMAINING_BU">
                <Label Caption="@Data.REMAINING_BU~" Format="###,###,##0" ID="colREMAINING_BU" />
                <DataColumnSort DataColumn="REMAINING_BU" DataType="Number" />
              </DataTableColumn>
              <DataTableColumn ID="colKPI">
                <Label ID="KPI">
                  <ConditionalClass Class="fa fa-arrow-up fa-2x text-success" Condition="@Data.REMAINING_BU~&gt; 1000" />
                  <ConditionalClass Class="fa fa-arrow-down fa-2x text-danger" Condition="@Data.REMAINING_BU~&lt; 0" />
                  <ConditionalClass Class="fa fa-minus fa-2x text-warning" Condition="@Data.REMAINING_BU~  &lt;= 1000" />
                </Label>
              </DataTableColumn>
            </DataTable>
          </Division>
          <Division Class="panel-body" HtmlDiv="True" ID="panel-body">
            <Remark>
              <ChartCanvas ID="PieChartCanvas1">
                <DataLayer ConnectionID="Northwind" ID="SQLDataLayer2" Source="Select Invoices.Salesperson,&#xD;&#xA;  Count(Invoices.OrderID)&#xD;&#xA;From Invoices&#xD;&#xA;WHERE YEAR(Invoices.OrderDate) = '@Request.Year~'&#xD;&#xA;Group By Invoices.Salesperson" Type="SQL">
                  <GroupFilter DataType="Text" GroupColumn="Salesperson" KeepGroupedRows="False">
                    <GroupAggregateColumn AggregateColumn="Column1" AggregateFunction="SUM" DataType="Number" ID="rdGrAggr-Column1-sum" />
                  </GroupFilter>
                  <SortFilter DataType="Number" SortColumn="rdGrAggr-Column1-sum" SortSequence="Descending" />
                  <RelevanceFilter DataColumn="rdGrAggr-Column1-sum" IrrelevanceLabel="Other" LabelColumn="Salesperson" RelevanceType="NumberOfRows" RelevanceValue="0" />
                </DataLayer>
