<?xml version="1.0" encoding="utf-8"?>
<Report ID="Home.MKCReport">
  <DefaultRequestParams paramCust="CHS INC" />
  <LocalData>
    <DataLayer ConnectionID="connProdGP" ID="SQLWheat" Source="Select IV.DESCRIPTION As COMMODITYDESC,&#xD;&#xA;  Sum((CM.ISSUED_LBS - CM.APPLIED_LBS + CM.ADJUSTED_LBS - CM.SUB_CONT_LBS) /&#xD;&#xA;  UOM.STANDARD_UNITS_LBS) As RemainBalanceWheat&#xD;&#xA;From CA_CONTRACT_MASTER As CM&#xD;&#xA;  Join CA_COMMODITY_MASTER As IV On CM.COMMODITY_CODE = IV.COMMODITY_CODE&#xD;&#xA;  Join IV_UNIT_OF_MEASURE As UOM On IV.UOM_BUSHELS = UOM.UNIT_OF_MEASURE&#xD;&#xA;Where CM.CONTRACT_STATUS = 'O' And CM.COMMODITY_CODE = '01'&#xD;&#xA;Group By IV.DESCRIPTION" Type="SQL" />
  </LocalData>
  <LocalData>
    <DataLayer ConnectionID="connProdGP" ID="SQLSoybean" Source="Select IV.DESCRIPTION As COMMODITYDESC,&#xD;&#xA;  Sum((CM.ISSUED_LBS - CM.APPLIED_LBS + CM.ADJUSTED_LBS - CM.SUB_CONT_LBS) /&#xD;&#xA;  UOM.STANDARD_UNITS_LBS) As RemainBalanceSoybean&#xD;&#xA;From CA_CONTRACT_MASTER As CM&#xD;&#xA;  Join CA_COMMODITY_MASTER As IV On CM.COMMODITY_CODE = IV.COMMODITY_CODE&#xD;&#xA;  Join IV_UNIT_OF_MEASURE As UOM On IV.UOM_BUSHELS = UOM.UNIT_OF_MEASURE&#xD;&#xA;Where CM.CONTRACT_STATUS = 'O' And CM.COMMODITY_CODE = '03'&#xD;&#xA;Group By IV.DESCRIPTION" Type="SQL" />
  </LocalData>
  <LocalData>
    <DataLayer ConnectionID="connProdGP" ID="SQLCorn" Source="Select IV.DESCRIPTION As COMMODITYDESC,&#xD;&#xA;  Sum((CM.ISSUED_LBS - CM.APPLIED_LBS + CM.ADJUSTED_LBS - CM.SUB_CONT_LBS) /&#xD;&#xA;  UOM.STANDARD_UNITS_LBS) As RemainBalanceCorn&#xD;&#xA;From CA_CONTRACT_MASTER As CM&#xD;&#xA;  Join CA_COMMODITY_MASTER As IV On CM.COMMODITY_CODE = IV.COMMODITY_CODE&#xD;&#xA;  Join IV_UNIT_OF_MEASURE As UOM On IV.UOM_BUSHELS = UOM.UNIT_OF_MEASURE&#xD;&#xA;Where CM.CONTRACT_STATUS = 'O' And CM.COMMODITY_CODE = '04'&#xD;&#xA;Group By IV.DESCRIPTION" Type="SQL" />
  </LocalData>
  <LocalData>
    <DataLayer ConnectionID="connProdGP" ID="SQLMilo" Source="Select IV.DESCRIPTION As COMMODITYDESC,&#xD;&#xA;  Sum((CM.ISSUED_LBS - CM.APPLIED_LBS + CM.ADJUSTED_LBS - CM.SUB_CONT_LBS) /&#xD;&#xA;  UOM.STANDARD_UNITS_LBS) As RemainBalanceMilo&#xD;&#xA;From CA_CONTRACT_MASTER As CM&#xD;&#xA;  Join CA_COMMODITY_MASTER As IV On CM.COMMODITY_CODE = IV.COMMODITY_CODE&#xD;&#xA;  Join IV_UNIT_OF_MEASURE As UOM On IV.UOM_BUSHELS = UOM.UNIT_OF_MEASURE&#xD;&#xA;Where CM.CONTRACT_STATUS = 'O' And CM.COMMODITY_CODE = '02'&#xD;&#xA;Group By IV.DESCRIPTION" Type="SQL" />
  </LocalData>
  <LocalData>
    <DataLayer ConnectionID="connMKC" ID="lContractsOvershipped" Source="Select SUBSTRING(GaugeAgTraxContracts.CONTRACTNMBR,1,CHARINDEX('.',GaugeAgTraxContracts.CONTRACTNMBR)-1) CONTRACTNMBR,&#xD;&#xA;  Sum(GaugeAgTraxContracts.REMAINING_BU) As REMAINING_BU,&#xD;&#xA;  GaugeAgTraxContracts.COMMODITYDESC&#xD;&#xA;From GaugeAgTraxContracts&#xD;&#xA;Where GaugeAgTraxContracts.REMAINING_BU &lt; 0&#xD;&#xA;Group By SUBSTRING(GaugeAgTraxContracts.CONTRACTNMBR,1,CHARINDEX('.',GaugeAgTraxContracts.CONTRACTNMBR)-1),&#xD;&#xA;  GaugeAgTraxContracts.COMMODITYDESC&#xD;&#xA;Order By REMAINING_BU&#xD;&#xA;" Type="SQL">
      <DataLayerLink ID="linkContractsOvershipped" />
    </DataLayer>
  </LocalData>
  <IncludeSharedElement DefinitionFile="_Layout.Shared" SharedElementID="shrHeader" />
  <DefaultRequestParams paramContract="@Local.CONTRACTNMBR~" />
  <Body>
    <ResponsiveRow CollisionBehavior="Overlap" SecurityRightID="Admin">
