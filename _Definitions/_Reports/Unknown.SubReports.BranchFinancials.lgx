															Formula="IIF(@Data.agg_pMTD~==0,0,@Data.agg_pGMTD~/@Data.agg_pMTD~)"
															ID="calc_pGMTDpct"
														/>
														<CalculatedColumn
															ErrorResult="0"
															Formula="IIF(@Data.agg_pYTD~==0,0,@Data.agg_pGYTD~/@Data.agg_pYTD~)"
