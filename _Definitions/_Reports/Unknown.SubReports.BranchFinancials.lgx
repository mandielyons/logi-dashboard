												<DataTable
													ID="dtApplication"
													SortArrows="True"
													>
													<DataLayer
														ConnectionID="connProdGP"
														ID="sqlApplication"
														Source="SELECT cMTD.LocGLStr, cMTD.ACCATNUM, cMTD.ACTDESCR, SUM(cMTD.cMTD)*-1 cMTD, SUM(cYTD.cYTD)*-1 cYTD, SUM(pMTD.pMTD)*-1 pMTD, SUM(pYTD.pYTD)*-1 pYTD
FROM             (SELECT ACTNUMBR_2 [LocGLStr], MNACSGMT, LTRIM(RTRIM(ACCATNUM)) ACCATNUM, LTRIM(RTRIM(ACTDESCR)) ACTDESCR, SUM(PERDBLNC) cMTD FROM MKCGP..slbAccountSummary WHERE YEAR1=(@Request.paramFiscYear~   ) AND PERIODID =@Request.paramFiscPeriod~ AND ACTNUMBR_2 IN(&apos;@Request.paramLocGLStr~&apos;) AND MNACSGMT IN(&apos;@Request.AcctListApplication~&apos;) GROUP BY ACTNUMBR_2, MNACSGMT, ACCATNUM, ACTDESCR) AS cMTD
 LEFT OUTER JOIN (SELECT ACTNUMBR_2 [LocGLStr], MNACSGMT, LTRIM(RTRIM(ACCATNUM)) ACCATNUM, LTRIM(RTRIM(ACTDESCR)) ACTDESCR, SUM(PERDBLNC) cYTD FROM MKCGP..slbAccountSummary WHERE YEAR1=(@Request.paramFiscYear~   ) AND PERIODID&lt;=@Request.paramFiscPeriod~ AND ACTNUMBR_2 IN(&apos;@Request.paramLocGLStr~&apos;) AND MNACSGMT IN(&apos;@Request.AcctListApplication~&apos;) GROUP BY ACTNUMBR_2, MNACSGMT, ACCATNUM, ACTDESCR) AS cYTD ON cMTD.[LocGLStr]=cYTD.[LocGLStr] AND cMTD.MNACSGMT=cYTD.MNACSGMT AND cMTD.ACTDESCR=cYTD.ACTDESCR
 LEFT OUTER JOIN (SELECT ACTNUMBR_2 [LocGLStr], MNACSGMT, LTRIM(RTRIM(ACCATNUM)) ACCATNUM, LTRIM(RTRIM(ACTDESCR)) ACTDESCR, SUM(PERDBLNC) pMTD FROM MKCGP..slbAccountSummary WHERE YEAR1=(@Request.paramFiscYear~ -1) AND PERIODID =@Request.paramFiscPeriod~ AND ACTNUMBR_2 IN(&apos;@Request.paramLocGLStr~&apos;) AND MNACSGMT IN(&apos;@Request.AcctListApplication~&apos;) GROUP BY ACTNUMBR_2, MNACSGMT, ACCATNUM, ACTDESCR) AS pMTD ON cMTD.[LocGLStr]=pMTD.[LocGLStr] AND cMTD.MNACSGMT=pMTD.MNACSGMT AND cMTD.ACTDESCR=pMTD.ACTDESCR
 LEFT OUTER JOIN (SELECT ACTNUMBR_2 [LocGLStr], MNACSGMT, LTRIM(RTRIM(ACCATNUM)) ACCATNUM, LTRIM(RTRIM(ACTDESCR)) ACTDESCR, SUM(PERDBLNC) pYTD FROM MKCGP..slbAccountSummary WHERE YEAR1=(@Request.paramFiscYear~ -1) AND PERIODID&lt;=@Request.paramFiscPeriod~ AND ACTNUMBR_2 IN(&apos;@Request.paramLocGLStr~&apos;) AND MNACSGMT IN(&apos;@Request.AcctListApplication~&apos;) GROUP BY ACTNUMBR_2, MNACSGMT, ACCATNUM, ACTDESCR) AS pYTD ON cMTD.[LocGLStr]=pYTD.[LocGLStr] AND cMTD.MNACSGMT=pYTD.MNACSGMT AND cMTD.ACTDESCR=pYTD.ACTDESCR
GROUP BY cMTD.LocGLStr, cMTD.MNACSGMT, cMTD.ACCATNUM, cMTD.ACTDESCR
ORDER BY LocGLStr, cMTD.MNACSGMT
"
														Type="SQL"
													/>
