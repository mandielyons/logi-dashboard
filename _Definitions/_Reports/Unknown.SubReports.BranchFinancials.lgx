  (SELECT ROUND(SUM(PERDBLNC),0)    FROM MKCGP..slbAccountSummary WHERE YEAR1=(@Request.paramFiscYear~ -1) AND PERIODID&lt;=@Request.paramFiscPeriod~ AND ACTNUMBR_2 IN(&apos;@Request.paramLocGLStr~&apos;) AND MNACSGMT BETWEEN &apos;8000&apos; AND &apos;8499&apos;) pYTD
union all
SELECT 5 as IntGroup,10 as IntSort, 1 as IntTotal, 0 as blnClick, &apos;&apos; as strTabName, &apos;Net Earnings (Loss)&apos; as [Description], 
  (SELECT ROUND(SUM(PERDBLNC),0)*-1 FROM MKCGP..slbAccountSummary WHERE YEAR1=(@Request.paramFiscYear~   ) AND PERIODID =@Request.paramFiscPeriod~ AND ACTNUMBR_2 IN(&apos;@Request.paramLocGLStr~&apos;) AND (MNACSGMT BETWEEN &apos;4300&apos; AND &apos;5999&apos; OR MNACSGMT BETWEEN &apos;7000&apos; AND &apos;8499&apos;)) cMTD,
  (SELECT ROUND(SUM(PERDBLNC),0)*-1 FROM MKCGP..slbAccountSummary WHERE YEAR1=(@Request.paramFiscYear~   ) AND PERIODID&lt;=@Request.paramFiscPeriod~ AND ACTNUMBR_2 IN(&apos;@Request.paramLocGLStr~&apos;) AND (MNACSGMT BETWEEN &apos;4300&apos; AND &apos;5999&apos; OR MNACSGMT BETWEEN &apos;7000&apos; AND &apos;8499&apos;)) cYTD, 
  (SELECT ROUND(SUM(PERDBLNC),0)*-1 FROM MKCGP..slbAccountSummary WHERE YEAR1=(@Request.paramFiscYear~ -1) AND PERIODID =@Request.paramFiscPeriod~ AND ACTNUMBR_2 IN(&apos;@Request.paramLocGLStr~&apos;) AND (MNACSGMT BETWEEN &apos;4300&apos; AND &apos;5999&apos; OR MNACSGMT BETWEEN &apos;7000&apos; AND &apos;8499&apos;)) pMTD,
  (SELECT ROUND(SUM(PERDBLNC),0)*-1 FROM MKCGP..slbAccountSummary WHERE YEAR1=(@Request.paramFiscYear~ -1) AND PERIODID&lt;=@Request.paramFiscPeriod~ AND ACTNUMBR_2 IN(&apos;@Request.paramLocGLStr~&apos;) AND (MNACSGMT BETWEEN &apos;4300&apos; AND &apos;5999&apos; OR MNACSGMT BETWEEN &apos;7000&apos; AND &apos;8499&apos;)) pYTD
ORDER BY IntSort;
