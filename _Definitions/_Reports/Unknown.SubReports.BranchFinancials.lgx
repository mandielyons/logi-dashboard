	<LocalData
		ID="LocalData-FiscalDates"
		>
		<Note
			Note="The Local Data section has to stay above the Default Request Parameters section"
		/>
		<DataLayer
			ConnectionID="connProdGP"
			ID="sqlFiscalDates"
			Source="SELECT 
  CASE 
    WHEN DATEPART(MONTH,GETDATE())&gt;2 THEN DATEPART(YEAR,GETDATE())+1 
  ELSE DATEPART(YEAR,GETDATE()) 
  END FISCYEAR
, CASE 
    WHEN DATEPART(DAY,GETDATE())&lt;5 THEN  /*--if less than five days into the month, use previous month (period)--*/
      CASE 
	    WHEN DATEPART(MONTH,DATEADD(MONTH,-1,GETDATE()))&gt;2 THEN DATEPART(MONTH,DATEADD(MONTH,-1,GETDATE()))-2 
	  ELSE DATEPART(MONTH,DATEADD(MONTH,-1,GETDATE()))+10 
      END
  ELSE 
      CASE 
	    WHEN DATEPART(MONTH,GETDATE())&gt;2 THEN DATEPART(MONTH,GETDATE())-2 
	  ELSE DATEPART(MONTH,GETDATE())+10 
	  END 
  END FISCPERIODID
"
			Type="SQL"
		/>
	</LocalData>
