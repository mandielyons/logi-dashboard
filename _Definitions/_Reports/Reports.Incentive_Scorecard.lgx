		</Remark>
		<ResponsiveRow>
			<ResponsiveColumn>
				<Division
					Class="panel panel-default"
					HtmlDiv="True"
					ID="panel-PODs"
					>
					<Division
						Class="panel-heading"
						HtmlDiv="True"
						ID="panel-heading-PODs"
						>
						<Label
							Caption="Incentive Plan Scorecard"
						/>
					</Division>
					<Division
						Class="panel-body"
						HtmlDiv="True"
						ID="panel-body-PODs"
						>
						<Rows
							Width="100"
							WidthScale="%"
							>
							<Row>
								<Column
									Class="ThemeAlignTop, ThemeAlignRight"
									ColSpan="12"
									ID="colExport"
									Width="100"
									WidthScale="%"
									>
									<IncludeSharedElement
										DefinitionFile="_Layout.Shared"
										SharedElementID="shrExcelExport"
										>
										<SharedElementParams
											DataTableID="dtScorecard"
											ReportName="Incentive Plan"
											Security=""
										/>
									</IncludeSharedElement>
								</Column>
							</Row>
							<Row>
								<Column
									Class="ThemeAlignTop"
									ID="colFilters"
									Width="300"
									WidthScale="px"
									>
									<Rows
										Width="90"
										WidthScale="%"
										>
										<Row>
											<Column
												Class="ThemeAlignRight"
												ID="colIcon"
												>
												<LineBreak/>
												<Label
													Class="fa fa-chevron-left"
													>
													<Action
														Display="Toggle"
														ElementID="colFilters,colFilters2"
														ID="hideFilters"
														ShowElementEffect="FadeIn"
														Type="ShowElement"
													/>
												</Label>
											</Column>
										</Row>
										<Row>
											<Column
												ID="colPodLocation"
												Width="100"
												WidthScale="%"
												>
												<Division
													Class="well"
													HtmlDiv="True"
													ID="divFilterPodLocation"
													>
													<InputSelectList
														Caption="PODs:"
														DefaultValue="@Request.PodLocation~"
														ID="PodLocation"
														InputRows="17"
														MultiSelect="False"
														OptionCaptionColumn="LocDesc"
														OptionValueColumn="LocDesc"
														>
														<EventHandler
															DhtmlEvent="onchange"
															ID="evtOnChange"
															>
															<Action
																ElementID="dtScorecard"
																ID="action-refresh-report"
																Type="RefreshElement"
																>
																<WaitPage/>
															</Action>
														</EventHandler>
														<DataLayer
															ColumnNamesRow="1"
															ExcelFile="@Request.ExcelFileLocation~"
															ExcelWorksheet="PODLookup"
															ID="dlExcel-PODs"
															Type="Excel"
															>
															<SortFilter
																DataType="Text"
																SortColumn="LocDesc"
															/>
														</DataLayer>
													</InputSelectList>
												</Division>
											</Column>
										</Row>
									</Rows>
								</Column>
								<Column
									Class="ThemeAlignTop"
									ID="colFilters2"
									ShowModes="None"
									Width="30"
									WidthScale="px"
									>
									<Rows
										Width="90"
										WidthScale="%"
										>
										<Row>
											<Column
												ID="colIcon"
												>
												<LineBreak/>
												<Label
													Class="fa fa-chevron-right"
													>
													<Action
														Display="Toggle"
														ElementID="colFilters,colFilters2"
														ID="showFilters"
														ShowElementEffect="FadeIn"
														Type="ShowElement"
													/>
												</Label>
											</Column>
										</Row>
									</Rows>
								</Column>
								<Column
									Class="ThemeAlignTop"
									ID="colContent"
									>
									<ResponsiveRow
										Class="ThemeTextSmall"
										CollisionBehavior="Wrap"
										ID="respContent"
										>
										<ResponsiveColumn
											Class="ThemeAlignCenter"
											ColspanExtraSmallScreen="12"
											ColspanLargeScreen="6"
											ColspanMediumScreen="5"
											ColspanSmallScreen="12"
											>
											<Division
												Class="ThemeAlignCenter"
												ID="div-scorecard-report"
												>
												<LineBreak
													LineCount="3"
												/>
												<DataTable
													Caption="Scorecard YTD - @Request.PodLocation~"
													ID="dtScorecard"
													Width="100"
													WidthScale="%"
													>
													<DataLayer
														CellRange="A1:S300"
														ColumnNamesRow="1"
														ExcelFile="@Request.ExcelFileLocation~"
														ExcelWorksheet="Location Scorecard"
														ID="ExcelScorecard"
														Type="Excel"
														>
														<CompareFilter
															CompareType="="
															CompareValue="@Request.PodLocation~"
															DataColumn="LocationDesc"
															DataType="Text"
															ID="FilterLocation"
														/>
														<CompareFilter
															CaseSensitive="False"
															CompareType="="
															CompareValue="1"
															DataColumn="Metric Active"
															DataType="Boolean"
															ID="FilterMetricActive"
														/>
													</DataLayer>
													<DataTableColumn
														Header="Metric"
														ID="colMetricDesc"
														>
														<Label
															Caption="@Data.Location Metric~"
															ID="lblMeasureDesc"
														/>
														<Remark>
															<DataColumnSort
																DataColumn="MetricDesc"
															/>
														</Remark>
													</DataTableColumn>
													<DataTableColumn
														ID="colMetricId"
														>
														<Gauge
															AlignmentHorizontal="Center"
															BalloonLabelFormat="@Data.NumberFormat~"
															ChartHeight="80"
															ChartWidth="300"
															FontBold="True"
															FontSize="Small"
															ID="BalloonBarGauge"
															PlotBackgroundColor="Transparent"
															StartValue="@Data.Budget_Min~"
															TickCount="@Data.TickCount~"
															TickLabelFontSize="Small"
															TickLabelFormat="@Data.NumberFormat~"
															Type="BalloonBar"
															Value="@Data.Actual_Performance~"
															>
															<GaugeRange
																Color="@Data.LowColor~"
																RangeEndValue="@Data.Budget_Target~"
															/>
															<GaugeRange
																Color="@Data.HighColor~"
																RangeEndValue="@Data.Budget_Max~"
															/>
														</Gauge>
													</DataTableColumn>
													<DataTableColumn
														Header="Min"
														ID="colBudget_Min"
														>
														<Label
															Caption="@Data.Budget_Min~"
															Format="@Data.NumberFormat~"
															ID="lblBudget_Min"
														/>
														<Remark>
															<DataColumnSort
																DataColumn="Budget_Min"
																DataType="Number"
															/>
														</Remark>
													</DataTableColumn>
													<DataTableColumn
														Header="Max"
														ID="colBudget_Max"
														>
														<Label
															Caption="@Data.Budget_Max~"
															Format="@Data.NumberFormat~"
															ID="lblBudget_Max"
														/>
														<Remark>
															<DataColumnSort
																DataColumn="Budget_Max"
																DataType="Number"
															/>
														</Remark>
													</DataTableColumn>
													<DataTableColumn
														Header="Target"
														ID="colBudget_Target"
														>
														<Label
															Caption="@Data.Budget_Target~"
															Format="@Data.NumberFormat~"
															ID="lblBudget_Target"
														/>
														<Remark>
															<DataColumnSort
																DataColumn="Budget_Target"
																DataType="Number"
															/>
														</Remark>
													</DataTableColumn>
													<DataTableColumn
														Header="Actual"
														ID="colActual_Performance"
														>
														<Label
															Caption="@Data.Actual_Performance~"
															Format="@Data.NumberFormat~"
															ID="lblActual_Performance"
														/>
														<Remark>
															<DataColumnSort
																DataColumn="Actual_Performance"
																DataType="Number"
															/>
														</Remark>
													</DataTableColumn>
													<DataTableColumn
														Header="Notes"
														ID="colNotes"
														>
														<Label
															Caption="@Data.Notes~"
															ID="lblNotes"
														/>
														<Remark>
															<DataColumnSort
																DataColumn="Notes"
															/>
														</Remark>
													</DataTableColumn>
												</DataTable>
											</Division>
										</ResponsiveColumn>
									</ResponsiveRow>
								</Column>
							</Row>
						</Rows>
					</Division>
				</Division>
			</ResponsiveColumn>
		</ResponsiveRow>
	</Body>
