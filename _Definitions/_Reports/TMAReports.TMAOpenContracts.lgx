									IncludeBlankValue="ALL"
									InputRows="1"
									MultiSelect="False"
									OptionCaptionColumn="DPR_POSITION"
									OptionValueColumn="DPR_POSITION"
									>
									<EventHandler
										DhtmlEvent="onchange"
										ID="evtOnChange"
										>
										<Action
											ElementID="div_content"
											ID="actRefreshReport"
											Type="RefreshElement"
											>
											<WaitPage/>
										</Action>
									</EventHandler>
									<DataLayer
										Type="Static"
										>
										<StaticDataRow
											DPR_POSITION="910"
										/>
										<StaticDataRow
											DPR_POSITION="920"
										/>
										<StaticDataRow
											DPR_POSITION="930"
										/>
										<StaticDataRow
											DPR_POSITION="940"
										/>
										<StaticDataRow
											DPR_POSITION="950"
										/>
									</DataLayer>
								</InputSelectList>
								<InputSelectList
									Caption="Customer"
									DefaultValue="@Request.PositionId~"
