						Condition="&quot;@Request.PositionId~&quot; = &quot;ALL&quot; OR &quot;@Data.DPR_POSITION~&quot; = &quot;@Request.PositionId~&quot;"
						ID="fltPositionId"
					/>
				</DataLayer>
