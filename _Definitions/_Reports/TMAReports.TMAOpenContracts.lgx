									ID="lblShowDown"
									>
									<Action
										Display="Toggle"
										ElementID="rowFilter1,rowFilter2"
										ID="actToggle"
										ShowElementEffect="FadeIn"
										Type="ShowElement"
									/>
								</Label>
							</Column>
						</Row>
