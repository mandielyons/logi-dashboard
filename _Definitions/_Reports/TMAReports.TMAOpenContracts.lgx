										>
										<StaticDataRow
											DPR_POSITION="910"
										/>
										<StaticDataRow
											DPR_POSITION="920"
										/>
										<StaticDataRow
											DPR_POSITION="930"
										/>
										<StaticDataRow
											DPR_POSITION="940"
										/>
										<StaticDataRow
											DPR_POSITION="950"
										/>
									</DataLayer>
								</InputSelectList>
							</Column>
						</Row>
