									MultiSelect="False"
									OptionCaptionColumn="DPR_POSITION"
									OptionValueColumn="DPR_POSITION"
									>
									<EventHandler
										DhtmlEvent="onchange"
										ID="evtOnChange"
										>
										<Action
											ElementID="div_content"
											ID="actRefreshReport"
											Type="RefreshElement"
											>
											<WaitPage/>
										</Action>
									</EventHandler>
