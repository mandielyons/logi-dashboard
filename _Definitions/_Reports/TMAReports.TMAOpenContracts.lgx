				AccessibleSummary="DataTable"
				ID="DataTable1"
				KeepScrollPosition="True"
				RememberSort="True"
				ResizableColumns="True"
				SortArrows="True"
				TableBorder="0"
				Width="100"
				WidthScale="%"
				>
				<DataLayer
					ID="lnkOpenContracts"
					LinkedDataLayerID="dllOpenContracts"
					Type="Linked"
					>
