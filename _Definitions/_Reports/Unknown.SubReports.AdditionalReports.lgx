									<Label
										Caption="Avoidable Discounts - Dollars"
									/>
								</Division>
								<Division
									Class="panel-body"
									HtmlDiv="True"
									ID="panel-bodyAvoidableDiscounts"
									>
									<Rows
										Width="100"
										WidthScale="%"
										>
										<Row>
											<Column>
												<Spaces
													Size="30"
												/>
											</Column>
											<Column>
												<DataTable
													ID="dtAvoidableDiscounts"
													SortArrows="True"
													>
													<HeaderRow
														HeaderPosition="Top"
														ID="rowCustomHeader"
														>
														<Column/>
														<Column
															Class="ThemeAlignCenter"
															ColSpan="2"
