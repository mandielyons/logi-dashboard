											<Column>
												<DataTable
													ID="dtApplicationOperator"
													SortArrows="True"
													>
													<DataLayer
														ConnectionID="connProdGP"
														ID="sqlApplicationOperator"
														Source="SELECT cMTD.AgLocationCode, LTRIM(RTRIM(cMTD.Operator)) Operator, ISNULL(cMTD.Acres,0) cMTD, ISNULL(cYTD.Acres,0) cYTD, ISNULL(pMTD.Acres,0) pMTD, ISNULL(pYTD.Acres,0) pYTD
FROM        (SELECT Users.LocationCode AgLocationCode, Orders.DoneByID, LTRIM(RTRIM(Users.FirstName))+&apos; &apos;+LTRIM(RTRIM(Users.LastName)) Operator, SUM(Orders.AcresSprayed) Acres FROM AgOS_MKC_Live..Orders Orders with(nolock) INNER JOIN AgOS_MKC_Live..Users Users with(nolock) ON Orders.DoneByID=Users.UserID WHERE Orders.Done=1 AND CASE WHEN DATEPART(MONTH,Orders.DoneDate)&lt;3 THEN DATEPART(YEAR,Orders.DoneDate) ELSE DATEPART(YEAR,Orders.DoneDate)+1 END=@Request.paramFiscYear~    AND CASE DATEPART(MONTH,Orders.DoneDate) WHEN 3 THEN 1 WHEN 4 THEN 2 WHEN 5 THEN 3 WHEN 6 THEN 4 WHEN 7 THEN 5 WHEN 8 THEN 6 WHEN 9 THEN 7 WHEN 10 THEN 8 WHEN 11 THEN 9 WHEN 12 THEN 10 WHEN 1 THEN 11 WHEN 2 THEN 12 END =@Request.paramFiscPeriod~ GROUP BY Users.LocationCode, Orders.DoneByID, Users.LastName, Users.FirstName) cMTD 
  LEFT JOIN (SELECT Users.LocationCode AgLocationCode, Orders.DoneByID, LTRIM(RTRIM(Users.FirstName))+&apos; &apos;+LTRIM(RTRIM(Users.LastName)) Operator, SUM(Orders.AcresSprayed) Acres FROM AgOS_MKC_Live..Orders Orders with(nolock) INNER JOIN AgOS_MKC_Live..Users Users with(nolock) ON Orders.DoneByID=Users.UserID WHERE Orders.Done=1 AND CASE WHEN DATEPART(MONTH,Orders.DoneDate)&lt;3 THEN DATEPART(YEAR,Orders.DoneDate) ELSE DATEPART(YEAR,Orders.DoneDate)+1 END=@Request.paramFiscYear~    AND CASE DATEPART(MONTH,Orders.DoneDate) WHEN 3 THEN 1 WHEN 4 THEN 2 WHEN 5 THEN 3 WHEN 6 THEN 4 WHEN 7 THEN 5 WHEN 8 THEN 6 WHEN 9 THEN 7 WHEN 10 THEN 8 WHEN 11 THEN 9 WHEN 12 THEN 10 WHEN 1 THEN 11 WHEN 2 THEN 12 END&lt;=@Request.paramFiscPeriod~ GROUP BY Users.LocationCode, Orders.DoneByID, Users.LastName, Users.FirstName) cYTD ON cMTD.AgLocationCode=cYTD.AgLocationCode AND cMTD.DoneByID=cYTD.DoneByID
  LEFT JOIN (SELECT Users.LocationCode AgLocationCode, Orders.DoneByID, LTRIM(RTRIM(Users.FirstName))+&apos; &apos;+LTRIM(RTRIM(Users.LastName)) Operator, SUM(Orders.AcresSprayed) Acres FROM AgOS_MKC_Live..Orders Orders with(nolock) INNER JOIN AgOS_MKC_Live..Users Users with(nolock) ON Orders.DoneByID=Users.UserID WHERE Orders.Done=1 AND CASE WHEN DATEPART(MONTH,Orders.DoneDate)&lt;3 THEN DATEPART(YEAR,Orders.DoneDate) ELSE DATEPART(YEAR,Orders.DoneDate)+1 END=@Request.paramFiscYear~ -1 AND CASE DATEPART(MONTH,Orders.DoneDate) WHEN 3 THEN 1 WHEN 4 THEN 2 WHEN 5 THEN 3 WHEN 6 THEN 4 WHEN 7 THEN 5 WHEN 8 THEN 6 WHEN 9 THEN 7 WHEN 10 THEN 8 WHEN 11 THEN 9 WHEN 12 THEN 10 WHEN 1 THEN 11 WHEN 2 THEN 12 END =@Request.paramFiscPeriod~ GROUP BY Users.LocationCode, Orders.DoneByID, Users.LastName, Users.FirstName) pMTD ON cMTD.AgLocationCode=pMTD.AgLocationCode AND cMTD.DoneByID=pMTD.DoneByID
  LEFT JOIN (SELECT Users.LocationCode AgLocationCode, Orders.DoneByID, LTRIM(RTRIM(Users.FirstName))+&apos; &apos;+LTRIM(RTRIM(Users.LastName)) Operator, SUM(Orders.AcresSprayed) Acres FROM AgOS_MKC_Live..Orders Orders with(nolock) INNER JOIN AgOS_MKC_Live..Users Users with(nolock) ON Orders.DoneByID=Users.UserID WHERE Orders.Done=1 AND CASE WHEN DATEPART(MONTH,Orders.DoneDate)&lt;3 THEN DATEPART(YEAR,Orders.DoneDate) ELSE DATEPART(YEAR,Orders.DoneDate)+1 END=@Request.paramFiscYear~ -1 AND CASE DATEPART(MONTH,Orders.DoneDate) WHEN 3 THEN 1 WHEN 4 THEN 2 WHEN 5 THEN 3 WHEN 6 THEN 4 WHEN 7 THEN 5 WHEN 8 THEN 6 WHEN 9 THEN 7 WHEN 10 THEN 8 WHEN 11 THEN 9 WHEN 12 THEN 10 WHEN 1 THEN 11 WHEN 2 THEN 12 END&lt;=@Request.paramFiscPeriod~ GROUP BY Users.LocationCode, Orders.DoneByID, Users.LastName, Users.FirstName) pYTD ON cMTD.AgLocationCode=pYTD.AgLocationCode AND cMTD.DoneByID=pYTD.DoneByID
ORDER BY AgLocationCode, Operator
"
