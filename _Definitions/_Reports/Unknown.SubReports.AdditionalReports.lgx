											<Column>
												<DataTable
													ID="dtApplicationRig"
													SortArrows="True"
													>
													<DataLayer
														ConnectionID="connProdGP"
														ID="sqlApplicationRig"
														Source="SELECT cMTD.AgLocationCode, LTRIM(RTRIM(cMTD.Rig)) Rig, ISNULL(cMTD.Acres,0) cMTD, ISNULL(cYTD.Acres,0) cYTD, ISNULL(pMTD.Acres,0) pMTD, ISNULL(pYTD.Acres,0) pYTD
FROM        (SELECT Objects2.LocationCode AgLocationCode, Orders.ApplicatorID, Objects2.Name Rig, SUM(Orders.AcresSprayed) Acres FROM AgOS_MKC_Live..Orders Orders with(nolock) INNER JOIN AgOS_MKC_Live..Objects Objects2 with(nolock) ON Orders.ApplicatorID=Objects2.ObjectID WHERE Orders.Done=1 AND CASE WHEN DATEPART(MONTH,Orders.DoneDate)&lt;3 THEN DATEPART(YEAR,Orders.DoneDate) ELSE DATEPART(YEAR,Orders.DoneDate)+1 END=@Request.paramFiscYear~    AND CASE DATEPART(MONTH,Orders.DoneDate) WHEN 3 THEN 1 WHEN 4 THEN 2 WHEN 5 THEN 3 WHEN 6 THEN 4 WHEN 7 THEN 5 WHEN 8 THEN 6 WHEN 9 THEN 7 WHEN 10 THEN 8 WHEN 11 THEN 9 WHEN 12 THEN 10 WHEN 1 THEN 11 WHEN 2 THEN 12 END =@Request.paramFiscPeriod~ GROUP BY Objects2.LocationCode, Orders.ApplicatorID, Objects2.Name) cMTD 
  LEFT JOIN (SELECT Objects2.LocationCode AgLocationCode, Orders.ApplicatorID, Objects2.Name Rig, SUM(Orders.AcresSprayed) Acres FROM AgOS_MKC_Live..Orders Orders with(nolock) INNER JOIN AgOS_MKC_Live..Objects Objects2 with(nolock) ON Orders.ApplicatorID=Objects2.ObjectID WHERE Orders.Done=1 AND CASE WHEN DATEPART(MONTH,Orders.DoneDate)&lt;3 THEN DATEPART(YEAR,Orders.DoneDate) ELSE DATEPART(YEAR,Orders.DoneDate)+1 END=@Request.paramFiscYear~    AND CASE DATEPART(MONTH,Orders.DoneDate) WHEN 3 THEN 1 WHEN 4 THEN 2 WHEN 5 THEN 3 WHEN 6 THEN 4 WHEN 7 THEN 5 WHEN 8 THEN 6 WHEN 9 THEN 7 WHEN 10 THEN 8 WHEN 11 THEN 9 WHEN 12 THEN 10 WHEN 1 THEN 11 WHEN 2 THEN 12 END&lt;=@Request.paramFiscPeriod~ GROUP BY Objects2.LocationCode, Orders.ApplicatorID, Objects2.Name) cYTD ON cMTD.AgLocationCode=cYTD.AgLocationCode AND cMTD.ApplicatorID=cYTD.ApplicatorID
  LEFT JOIN (SELECT Objects2.LocationCode AgLocationCode, Orders.ApplicatorID, Objects2.Name Rig, SUM(Orders.AcresSprayed) Acres FROM AgOS_MKC_Live..Orders Orders with(nolock) INNER JOIN AgOS_MKC_Live..Objects Objects2 with(nolock) ON Orders.ApplicatorID=Objects2.ObjectID WHERE Orders.Done=1 AND CASE WHEN DATEPART(MONTH,Orders.DoneDate)&lt;3 THEN DATEPART(YEAR,Orders.DoneDate) ELSE DATEPART(YEAR,Orders.DoneDate)+1 END=@Request.paramFiscYear~ -1 AND CASE DATEPART(MONTH,Orders.DoneDate) WHEN 3 THEN 1 WHEN 4 THEN 2 WHEN 5 THEN 3 WHEN 6 THEN 4 WHEN 7 THEN 5 WHEN 8 THEN 6 WHEN 9 THEN 7 WHEN 10 THEN 8 WHEN 11 THEN 9 WHEN 12 THEN 10 WHEN 1 THEN 11 WHEN 2 THEN 12 END =@Request.paramFiscPeriod~ GROUP BY Objects2.LocationCode, Orders.ApplicatorID, Objects2.Name) pMTD ON cMTD.AgLocationCode=pMTD.AgLocationCode AND cMTD.ApplicatorID=pMTD.ApplicatorID
  LEFT JOIN (SELECT Objects2.LocationCode AgLocationCode, Orders.ApplicatorID, Objects2.Name Rig, SUM(Orders.AcresSprayed) Acres FROM AgOS_MKC_Live..Orders Orders with(nolock) INNER JOIN AgOS_MKC_Live..Objects Objects2 with(nolock) ON Orders.ApplicatorID=Objects2.ObjectID WHERE Orders.Done=1 AND CASE WHEN DATEPART(MONTH,Orders.DoneDate)&lt;3 THEN DATEPART(YEAR,Orders.DoneDate) ELSE DATEPART(YEAR,Orders.DoneDate)+1 END=@Request.paramFiscYear~ -1 AND CASE DATEPART(MONTH,Orders.DoneDate) WHEN 3 THEN 1 WHEN 4 THEN 2 WHEN 5 THEN 3 WHEN 6 THEN 4 WHEN 7 THEN 5 WHEN 8 THEN 6 WHEN 9 THEN 7 WHEN 10 THEN 8 WHEN 11 THEN 9 WHEN 12 THEN 10 WHEN 1 THEN 11 WHEN 2 THEN 12 END&lt;=@Request.paramFiscPeriod~ GROUP BY Objects2.LocationCode, Orders.ApplicatorID, Objects2.Name) pYTD ON cMTD.AgLocationCode=pYTD.AgLocationCode AND cMTD.ApplicatorID=pYTD.ApplicatorID
ORDER BY AgLocationCode, Rig
"
														Type="SQL"
														>
														<Join
															ID="joinSqlToXL"
															IncludeCondition="True"
															JoinType="InnerJoin"
															SortLeftSide="True"
															SortRightSide="True"
															>
															<MatchCondition
																DataType="Text"
																ID="joinAgLocationCode"
																LeftDataColumn="AgLocationCode"
																RightDataColumn="AgLocationCode"
															/>
															<DataLayer
																ColumnNamesRow="1"
																ExcelFile="C:\inetpub\wwwroot\MKCoop\UploadedFiles\LocationData.xlsx"
																ExcelWorksheet="BranchData"
																ID="xlAgWorksLocation"
																Type="Excel"
																>
																<ConditionFilter
																	Condition="&apos;@Request.paramLocGLStr~&apos; == &apos;@Data.LocGLStr~&apos;"
																	ID="condLocGLStr"
																/>
															</DataLayer>
														</Join>
													</DataLayer>
													<DataTableColumn
														Class="ThemeMargin"
														ColumnHeaderClass="ThemeAlignCenter"
														Header="Rig Name"
														HeaderType="Text"
														ID="colRig"
														Width="25"
